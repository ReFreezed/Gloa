!run {
	!import "compiler"

	local options      = getBuildOptions()
	options.outputType = BuildOutputType.LUA_MODULE
	setBuildOptions(options)
}

local someFunc :: () {
	!import "basic"
	print("So funky!")
}

local round :: !import"math".round

local Module :: struct {
	foo:   string           = NULL,
	func:  ()               = NULL,
	round: (x:float) -> int = NULL,
}
export cast(Module) {
	foo   = "bar";
	func  = someFunc;
	round = round;
}
